{
  "name": "delete-empty",
  "version": "0.1.1",
  "description": "Recursively delete all empty folders in a directory and child directories.",
  "homepage": "https://github.com/jonschlinkert/delete-empty",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/delete-empty",
  "bugs": {
    "url": "https://github.com/jonschlinkert/delete-empty/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async": "^1.5.0",
    "delete": "^0.3.0",
    "lazy-cache": "^1.0.3",
    "matched": "^0.4.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "del",
    "delete",
    "dir",
    "directory",
    "empty",
    "files",
    "folder",
    "recurse",
    "recursive",
    "remove"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "reflinks": [
      "verb"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "copy",
        "delete",
        "fs-utils",
        "matched"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
